{
  "id": "tpl_fitness_001",
  "name": "AnotherLevel - Training Lead Capture",
  "slug": "anotherlevel-lead",
  "industry": "fitness",
  "description": "Professional fitness trainer lead capture page for football and track training programs",
  "theme": {
    "useSiteDefaults": true,
    "overrides": {
      "palette": {
        "primary": "#000000",
        "accent": "#10b981"
      }
    }
  },
  "components": [
    {
      "id": "c_header_1",
      "type": "header",
      "version": "1.0.0",
      "props": {
        "logo": {
          "type": "text",
          "content": "AnotherLevel",
          "size": "lg"
        },
        "navigation": [
          {
            "label": "Programs",
            "href": "#programs"
          },
          {
            "label": "About",
            "href": "#about"
          },
          {
            "label": "Contact",
            "href": "#contact"
          }
        ],
        "showThemeToggle": false,
        "sticky": true,
        "background": "glass"
      }
    },
    {
      "id": "c_hero_1",
      "type": "hero",
      "version": "1.0.0",
      "props": {
        "title": "Train like a pro",
        "subtitle": "Football & track-focused training",
        "description": "Elite athletic performance training designed for serious football and track athletes. Take your game to the next level with proven training methods.",
        "media": {
          "type": "image",
          "assetId": "assets/hero_train.jpg",
          "alt": "Professional athlete training session"
        },
        "cta": {
          "label": "Book Free Consult",
          "action": {
            "type": "open_form",
            "target": "form_lead_1"
          },
          "style": "primary",
          "size": "lg"
        },
        "background": "gradient",
        "layout": "split",
        "animation": true
      },
      "analytics": {
        "trackClicks": true,
        "trackViews": true
      }
    },
    {
      "id": "c_feature_grid_1",
      "type": "feature_grid",
      "version": "1.0.0",
      "props": {
        "title": "Training Programs",
        "description": "Specialized training designed for football and track athletes",
        "features": [
          {
            "icon": "Zap",
            "title": "Speed Development",
            "description": "Explosive speed training for track and field events",
            "badge": "Popular"
          },
          {
            "icon": "Target",
            "title": "Football Skills",
            "description": "Position-specific training for football players",
            "badge": "Pro"
          },
          {
            "icon": "Trophy",
            "title": "Competition Prep",
            "description": "Mental and physical preparation for competition"
          }
        ],
        "columns": 3,
        "layout": "cards",
        "spacing": "normal"
      }
    },
    {
      "id": "form_lead_1",
      "type": "form",
      "version": "1.0.0",
      "props": {
        "title": "Start Your Training Journey",
        "description": "Get a free consultation to discuss your athletic goals and training needs",
        "fields": [
          {
            "id": "f1",
            "name": "name",
            "type": "text",
            "label": "Full Name",
            "placeholder": "Enter your full name",
            "required": true,
            "validation": {
              "minLength": 2,
              "maxLength": 50
            }
          },
          {
            "id": "f2",
            "name": "email",
            "type": "email",
            "label": "Email Address",
            "placeholder": "your.email@example.com",
            "required": true
          },
          {
            "id": "f3",
            "name": "phone",
            "type": "tel",
            "label": "Phone Number",
            "placeholder": "(555) 123-4567",
            "required": true
          },
          {
            "id": "f4",
            "name": "sport",
            "type": "select",
            "label": "Primary Sport",
            "required": true,
            "options": [
              {
                "label": "Football",
                "value": "football"
              },
              {
                "label": "Track & Field",
                "value": "track"
              },
              {
                "label": "Both",
                "value": "both"
              },
              {
                "label": "Other",
                "value": "other"
              }
            ]
          },
          {
            "id": "f5",
            "name": "experience",
            "type": "select",
            "label": "Experience Level",
            "required": true,
            "options": [
              {
                "label": "Beginner (0-1 years)",
                "value": "beginner"
              },
              {
                "label": "Intermediate (2-5 years)",
                "value": "intermediate"
              },
              {
                "label": "Advanced (5+ years)",
                "value": "advanced"
              }
            ]
          },
          {
            "id": "f6",
            "name": "goals",
            "type": "textarea",
            "label": "Training Goals",
            "placeholder": "Tell us about your specific training goals and what you want to achieve",
            "required": false,
            "validation": {
              "maxLength": 500
            }
          }
        ],
        "submission": {
          "type": "webhook",
          "target": "https://hooks.anotherlevel.com/lead",
          "method": "POST"
        },
        "layout": "vertical",
        "options": {
          "spamProtection": true,
          "requireConsent": false
        }
      }
    },
    {
      "id": "c_cta_1",
      "type": "cta",
      "version": "1.0.0",
      "props": {
        "title": "Ready to elevate your performance?",
        "description": "Join elite athletes who train with AnotherLevel",
        "buttons": [
          {
            "label": "Schedule Consultation",
            "action": {
              "type": "open_form",
              "target": "form_lead_1"
            },
            "style": "primary",
            "size": "lg"
          },
          {
            "label": "View Programs",
            "action": {
              "type": "scroll_to",
              "target": "#programs"
            },
            "style": "outline",
            "size": "lg"
          }
        ],
        "background": "gradient",
        "alignment": "center",
        "spacing": "lg"
      }
    }
  ],
  "meta": {
    "version": "1.0.0",
    "createdBy": "team_dct",
    "createdAt": "2025-09-17",
    "tags": ["fitness", "lead-capture", "athletics", "training"],
    "previewUrl": "https://preview.automationdct.com/tpl_fitness_001"
  }
}