  "questionnaire": {
    "id": "universal-consultation-intake",
    "title": "Universal Business Analysis",
    "description": "AI-powered questionnaire that adapts to your industry and business needs",
    "steps": [
      {
        "id": "business-basics",
        "title": "Business Profile",
        "questions": [
          {
            "id": "company-name",
            "text": "What's your company name?",
            "type": "short-text",
            "required": true,
            "placeholder": "Your Company Name"
          },
          {
            "id": "industry",
            "text": "What industry are you in?",
            "type": "chips",
            "required": true,
            "options": [
              { "value": "technology", "label": "Technology/Software" },
              { "value": "healthcare", "label": "Healthcare" },
              { "value": "finance", "label": "Financial Services" },
              { "value": "retail", "label": "Retail/E-commerce" },
              { "value": "manufacturing", "label": "Manufacturing" },
              { "value": "consulting", "label": "Consulting" },
              { "value": "education", "label": "Education" },
              { "value": "nonprofit", "label": "Non-profit" },
              { "value": "other", "label": "Other" }
            ],
            "allowCustom": true
          },
          {
            "id": "company-size",
            "text": "How many employees do you have?",
            "type": "select",
            "required": true,
            "options": [
              { "value": "solo", "label": "Just me (solo)" },
              { "value": "small", "label": "2-10 employees" },
              { "value": "medium", "label": "11-50 employees" },
              { "value": "large", "label": "51-200 employees" },
              { "value": "enterprise", "label": "200+ employees" }
            ]
          }
        ]
      },
      {
        "id": "challenges",
        "title": "Current Challenges",
        "questions": [
          {
            "id": "primary-challenges",
            "text": "What are your biggest business challenges right now?",
            "type": "chips-multi",
            "required": true,
            "options": [
              { "value": "growth", "label": "Scaling/Growth" },
              { "value": "efficiency", "label": "Operational Efficiency" },
              { "value": "technology", "label": "Technology/Digital Transformation" },
              { "value": "team", "label": "Team Management" },
              { "value": "marketing", "label": "Marketing/Customer Acquisition" },
              { "value": "finance", "label": "Financial Management" },
              { "value": "competition", "label": "Competitive Pressure" },
              { "value": "compliance", "label": "Compliance/Regulatory" }
            ],
            "allowCustom": true
          },
          {
            "id": "urgency",
            "text": "How urgent is solving these challenges?",
            "type": "select",
            "required": true,
            "options": [
              { "value": "immediate", "label": "Immediate (within 30 days)" },
              { "value": "short-term", "label": "Short-term (1-3 months)" },
              { "value": "medium-term", "label": "Medium-term (3-6 months)" },
              { "value": "long-term", "label": "Long-term (6+ months)" }
            ]
          }
        ]
      },
      {
        "id": "goals",
        "title": "Goals & Objectives",
        "questions": [
          {
            "id": "primary-goals",
            "text": "What are your primary business goals?",
            "type": "chips-multi",
            "required": true,
            "options": [
              { "value": "revenue-growth", "label": "Revenue Growth" },
              { "value": "cost-reduction", "label": "Cost Reduction" },
              { "value": "market-expansion", "label": "Market Expansion" },
              { "value": "product-development", "label": "Product Development" },
              { "value": "team-building", "label": "Team Building" },
              { "value": "automation", "label": "Process Automation" },
              { "value": "customer-satisfaction", "label": "Customer Satisfaction" },
              { "value": "digital-transformation", "label": "Digital Transformation" }
            ],
            "allowCustom": true
          },
          {
            "id": "success-metrics",
            "text": "How do you measure success?",
            "type": "long-text",
            "required": false,
            "placeholder": "Describe your key performance indicators and success metrics..."
          }
        ]
      }
